---
const response = await fetch(
  "https://opensheet.elk.sh/1AHqEDEJr_dmTWN5_JEZf0QcO7mLFeMgnimGyCWk5NGs/1",
);
const data = await response.json();
---

<section>
  <h3>2023 Results</h3>


  <table>
    <tr>
      <th>Rider</th><th>Course</th><th>Time</th><th>Club</th><th>Gender</th><th
        >Category</th><th>Laps</th><th>Date</th>
      
    </tr>

    {
      data.map((result) => (
        <tr>
          <td>
            <p>{result.Rider}</p>
          </td>
          <td>
            <p>{result.Course}</p>
          </td>
          <td>
            <p>{result.Time}</p>
          </td>
          <td>
            <p>{result.Club}</p>
          </td>
          <td>
            <p>{result.Gender}</p>
          </td>
          <td>
            <p>{result.Category}</p>
          </td>
          <td>
            <p>{result.Laps}</p>
          </td>
          <td>
            <p>{result.Date}</p>
          </td>
        </tr>
      ))
    }
  </table>
</section>
